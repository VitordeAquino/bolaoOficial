
<div class="row">
  <div class="col-xs-6 col-md-4"></div>
  <div class="col-xs-6 col-md-4">
	<div class="jumbotron">
		
	  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
			<%= devise_error_messages! %>
			<h2>Registrar</h2>

			<p><div><%= f.label :nome %><br />
		  <%= f.text_field :nome %></div></p>

		  <p><div><%= f.label :email %><br />
		  <%= f.email_field :email, :autofocus => true %></div></p>

		  <p><div><%= f.label :senha %></div>
		  <div><%= f.password_field :password %></div></p>

		  <p><div><%= f.label :confirmaÃ§Ã£o_de_senha %>
		  <%= f.password_field :password_confirmation %></div></p>

		  </p><div><%= f.submit "Registrar", class: "btn btn-primary btn-lg" %></div></p>

		  <%= render "devise/shared/links" %>
		<% end %>
	</div>
  </div>
  <div class="col-xs-6 col-md-4"></div>
</div>




